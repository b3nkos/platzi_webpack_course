{
  "name": "cursowebpackplatzi",
  "version": "1.0.0",
  "description": "Platzi course of webpack",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode='development' index.js --output bundle.js",
    "build:dev": "webpack",
    "build:external": "webpack --config=./external/webpack.config.js",
    "build:css": "webpack --config=./css-style-loader/webpack.config.js",
    "build:extract-css": "webpack --config=./plugin-extract-text/webpack.config.js",
    "build:multiple-entry-points": "webpack --config=./multiple-entry-points/webpack.config.js",
    "build:webpack-dev-server": "webpack-dev-server --config=./webpack-dev-server/webpack.config.js",
    "build:babel-loader": "webpack --config=./babel-loader/webpack.config.js",
    "build:url-loader-images": "webpack --config=./url-loader-images/webpack.config.js",
    "build:url-loader-fonts": "webpack --config=./url-loader-fonts/webpack.config.js",
    "build:url-url-loader-video": "webpack --config=./url-loader-video/webpack.config.js",
    "build:json-loader": "webpack --config=./json-loader/webpack.config.js",
    "build:react": "webpack --config=./react/webpack.config.js"
  },
  "keywords": [
    "webpack"
  ],
  "author": "Cristian Mart√≠nez",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.51",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.51",
    "@babel/preset-env": "^7.0.0-beta.51",
    "@babel/preset-react": "^7.0.0-beta.51",
    "@babel/preset-stage-2": "^7.0.0-beta.51",
    "@babel/runtime": "^7.0.0-beta.51",
    "babel-loader": "^8.0.0-beta.4",
    "css-loader": "0.28.11",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "mini-css-extract-plugin": "^0.4.1",
    "style-loader": "0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "4.12.2",
    "webpack-cli": "3.0.8",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "react": "^16.4.1",
    "react-dom": "^16.4.1"
  }
}
